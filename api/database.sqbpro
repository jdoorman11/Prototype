PRAGMA foreign_keys = OFF;        -- tijdelijk uitschakelen



-- 1️⃣ Nieuwe tabel zonder parent_id

CREATE TABLE Documents_new (

    doc_id           INTEGER PRIMARY KEY,

    document_name    TEXT,

    tag              TEXT,

    publication_date DATE,

    status           TEXT,

    content          TEXT

);



-- 2️⃣ Gegevens overzetten

INSERT INTO Documents_new (doc_id, document_name, tag, publication_date, status, content)

SELECT                    doc_id, document_name, tag, publication_date, status, content

FROM   Documents;



-- 3️⃣ Oude tabel weg &amp; nieuwe hernoemen

DROP TABLE Documents;

ALTER TABLE Documents_new RENAME TO Documents;



PRAGMA foreign_keys = ON;         -- referentiële integriteit weer aan

